msgstr ""

#. Key: KeywordDefinition.Shop_Crafting.DisplayName
msgctxt "KeywordDefinition.Shop_Crafting.DisplayName"
msgid "Crafting"
msgstr ""

#. Key: KeywordDefinition.Shop_Crafting.Description
msgctxt "KeywordDefinition.Shop_Crafting.Description"
