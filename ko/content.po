msgid "Relaxation options to raise the <slink=Mood> of your Mages."
msgstr ""

#. Key: KeywordDefinition.Shop_Decorations.DisplayName
msgctxt "KeywordDefinition.Shop_Decorations.DisplayName"
msgid "Decorations"
msgstr ""

#. Key: KeywordDefinition.Shop_Decorations.Description
msgctxt "KeywordDefinition.Shop_Decorations.Description"
msgid "Finery that increases the <slink=Luxury> of your school."
msgstr ""

